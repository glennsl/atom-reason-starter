{
  "name": "reasonable-atom-starter",
  "main": "./lib/js/lib/main",
  "version": "0.0.0",
  "description": "A very rough starter project for atom packages written in Reason and BuckleScript",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": "reasonable-atom-starter:toggle"
  },
  "repository": "https://github.com/glennsl/reasonable-atom-starter",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "postinstall": "ln -s \"$(pwd)\" ~/.atom/packages/",
    "start": "bsb -make-world -w"
  },
  "dependencies": {
    "bs-atom": "github:glennsl/bs-atom",
    "reason-js": "BuckleTypes/bs-webapi-incubator",
    "rebase": "glennsl/rebase"
  },
  "devDependencies": {
    "bs-platform": "bloomberg/bucklescript"
  }
}
